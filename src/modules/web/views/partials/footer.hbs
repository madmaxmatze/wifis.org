<footer class="py-5 container">
    <div class="container-lg">
        <div class="row">
            <div class="col-6 col-md-3 mb-4">
                <h5>Section</h5>
                <ul class="nav flex-column">
                    <li class="nav-item mb-2">
                        <a href="/{{getLocale}}/about" class="nav-link p-0 text-muted">
                            {{__ "about.linklabel"}}
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a href="/{{getLocale}}/press"
                            class="nav-link p-0 text-muted {{#if (equals pageId "press")}}class=" active" {{/if}}">
                            {{__ "press.linklabel"}}
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a href="/{{getLocale}}/tos" class="nav-link p-0 text-muted">
                            {{__ "tos.linklabel"}}
                        </a>
                    </li>
                    <li class="nav-item mb-2">
                        <a href="/{{getLocale}}/languages" class="nav-link p-0 text-muted">
                            {{__ "languages.linklabel"}}
                        </a>
                    </li>

                </ul>
            </div>

            <div class="col-6 col-md-2 mb-3">
                <h5>Blog</h5>
                <ul class="nav flex-column">
                    <li class="nav-item mb-2">
                        <a href="https://blog.wifis.org" class="nav-link p-0 text-muted">{{__ "blog.linklabel"}}</a>
                    </li>
                </ul>
            </div>

            <div class="col-md-5 offset-md-1 mb-3">
                <form>
                    <h5>Subscribe to our newsletter</h5>
                    <p>Monthly digest of what's new and exciting from us.</p>
                    <div class="d-flex flex-column flex-sm-row w-100 gap-2">
                        <label for="newsletter1" class="visually-hidden">Email address</label>
                        <input id="newsletter1" type="text" class="form-control" placeholder="Email address">
                        <button class="btn btn-primary" type="button">Subscribe</button>
                    </div>
                </form>
            </div>
        </div>

        <div class="d-flex flex-column flex-sm-row justify-content-between py-4 border-top">
            <p>&copy; 2023 wifis.org, all rights reserved</p>
            <ul class="list-unstyled d-flex">
                <li class="ms-3">
                    <a class="link-dark icon twitter_icon" href="https://twitter.com/wifis_org">
                        <span>twitter</span>
                    </a>
                </li>
                <li class="ms-3">
                    <a class="link-dark icon facebook_icon" href="https://www.facebook.com/wifis.org/">
                        <span>facebook</span>
                    </a>
                </li>
                <li class="ms-3">
                    <a class="link-dark icon github_icon" href="https://github.com/madmaxmatze/wifis.org">
                        <span>github</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</footer>