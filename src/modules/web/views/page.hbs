{{#with (lookup translations pageId)}}
    {{#each content}}
        {{#if this.headline}}
            <h3>{{{this.headline}}}</h3>
        {{/if}}
        <div class="indent">
            {{#if this.subHeadline}}
                <h4>{{{this.subHeadline}}}</h4>
            {{/if}}
            {{#each this.text}}
                <p>
                    {{#replace "[ABOUT_TEAM]" "<a href='https://mathiasnitzsche.de' target='_blank'>Mathias</a>
                        <span style='color: #BBB'>(Founder, Coder),</span>
                        
                        <a href='https://www.linkedin.com/in/hoehne/' target='_blank'>Robert</a>
                        <span style='color: #BBB'>(Product),</span>
                        
                        Malena <span style='color: #BBB'>(Marketing/User Support),</span>
                        
                        <a href='https://vonverhille.tumblr.com/' target='_blank'>James</a>
                        <span style='color: #BBB'>(Design)</span>"
                    "[ABOUT_SUPPORTER]" "Howard <span style='color: #BBB'>(ES-Translation),</span>
                        <a href='https://www.facebook.com/karenkennes22' target='_blank'>Karen</a>
                        <span style='color: #BBB'>(NL-Translation),</span>
                        <a href='https://www.linkedin.com/in/lorenzotubertini'
                            target='_blank'>Lorenzo</a>
                        <span style='color: #BBB'>(IT-Translation),</span>
                        <a href='https://jobberies.com' target='_blank'>Muhaimin</a>
                        <span style='color: #BBB'>(MS-Translation),</span>
                        <a href='https://ravenstalker.wordpress.com' target='_blank'>ArnO</a>
                        <span style='color: #BBB'>&amp;</span>
                        <a href='https://twitter.com/russelomua' target='_blank'>Николай</a>
                        <span style='color: #BBB'>(RU-Translation)</span>"
                    "[CONTACT_MAIL_ICON]" "<span class='sprite icon32 icon32-mail'></span>"
                    "[CONTACT_MAIL_ADDRESS]" "<b>&#105;&#110;&#102;&#111;&#64;&#119;&#105;&#102;&#105;&#115;&#46;&#111;&#114;&#103;</b>"
                    "[CONTACT_WIFI_ICON]" "<a href='/contact' class='sprite icon32 icon32-wifi'></a>"
                    "[CONTACT_WIFI_ADDRESS]" "<a href='/contact'>wifis.org/contact</a>"
                    "[CONTACT_TWITTER_ICON]" "<a href='https://twitter.com/wifis_org' target='_blank' class='sprite icon32 icon32-twitter'></a>"
                    "[CONTACT_TWITTER_ADDRESS]" "<a href='https://twitter.com/wifis_org' target='_blank'>Twitter</a>"
                    "[CONTACT_FACEBOOK_ICON]" "<a href='https://www.facebook.com/wifis.org' target='_blank' class='sprite icon32 icon32-facebook'></a>"
                    "[CONTACT_FACEBOOK_ADDRESS]" "<a href='https://www.facebook.com/wifis.org' target='_blank'>Facebook</a>"
                    "[CONTACT_BLOG_ICON]" "<a href='https://blog.wifis.org' target='_blank' class='sprite icon32 icon32-tumblr'></a>"
                    "[CONTACT_BLOG_ADDRESS]" "<a href='https://blog.wifis.org' target='_blank'>Blog</a>"
                    "[TOS]" (../../../__ "tos.termsOfService")}}
                        {{{../../__ this}}}
                    {{/replace}}
                </p>
            {{/each}}
        </div>
    {{/each}}
{{/with}}